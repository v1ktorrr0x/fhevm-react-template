{
  "buildCommand": "cd packages/nextjs && pnpm build",
  "devCommand": "cd packages/nextjs && pnpm dev",
  "installCommand": "corepack enable pnpm && corepack install -g pnpm@9.12.3 && pnpm install",
  "framework": "nextjs",
  "outputDirectory": "packages/nextjs/.next",
  "git": {
    "deploymentEnabled": {
      "vercel-deployment": true
    }
  }
}
